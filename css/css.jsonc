	"label required asterisk": {
		"prefix": "label required asterisk",
		"body": [
			"label.required::after {",
			"\tcontent: \" *\";",
			"\tcolor: #e32;",
			"\tvertical-align: middle; ",
			"}"
		],
		"description": "Adds a red asterisk on labels with required fields."
