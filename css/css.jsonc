{
	".noTextSelect - Class": {
		"prefix": ".no-text-select - Class",
		"body": [
			".no-text-select {",
			"\t/* iOS Safari */",
			"\t-webkit-touch-callout: none; ",
			"\t/* Safari */",
			"\t-webkit-user-select: none; ",
			"\t/* Konqueror HTML */",
			"\t-khtml-user-select: none; ",
			"\t/* Old versions of Firefox */",
			"\t-moz-user-select: none; ",
			"\t/* Internet Explorer/Edge */",
			"\t-ms-user-select: none; ",
			"\t/* Non-prefixed version, currently supported by Chrome, Edge, Opera and Firefox */",
			"\tuser-select: none; ",
			"}"
		],
		"description": "Disables the ability to select text."
